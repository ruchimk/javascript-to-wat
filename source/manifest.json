{
  "manifest_version": 2,
  "name": "Javascript to Wat",
  "version": "2.0",
  "description": "Replaces the text 'javascript' with 'wat'.",
  "icons": {
    "128": "icon.png"
  },
"content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["wat.js"],
      "run_at": "document_end"
    }
  ]
}
